          "_customHTML": "<section mbr-id=\"_three_in_one_form\" class=\"mbr-section three_in_one_form\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <input type=\"color\" title=\"Background Color\" name=\"bgColor\" value=\"#ffffff\">\n      <input type=\"checkbox\" title=\"Show logo\" name=\"logo\">\n      <select title=\"Form Type\" name=\"ftype\">\n            <option value=\"1\" selected>Signup</option>\n            <option value=\"2\">Login</option>\n            <option value=\"3\">Forget password</option>\n        </select>\n        <input type=\"text\" title=\"Form Action\" name=\"txtAction\" value=\"./processor.php\">\n        <input type=\"text\" title=\"Success Page\" name=\"txtSuccess\" value=\"./dashboard.php\">\n        <input type=\"text\" title=\"Error Page\" name=\"txtFail\" value=\"./error.php\">\n      <input type=\"text\" title=\"Database Folder path\" name=\"txtDb\" value=\"./database_folder\">\n      <hr><header>Form input settings</header>\n      <input type=\"checkbox\" title=\"Show Username\" name=\"username\" checked>\n      <input type=\"checkbox\" title=\"Show Fullname\" name=\"fullname\" checked>\n      <input type=\"checkbox\" title=\"Show Email\" name=\"email\" checked>\n      <input type=\"checkbox\" title=\"Show Gender\" name=\"gender\" checked>\n      <input type=\"checkbox\" title=\"Show Phone\" name=\"phone\" checked>\n      <input type=\"checkbox\" title=\"Show Password\" name=\"password\" checked>\n      <input type=\"checkbox\" title=\"Show Confirm Pass\" name=\"cpassword\" checked>\n    <!-- End block parameters -->\n    </mbr-parameters> \n\n    <!-- Navbar-->\n<header class=\"header\" mbr-if=\"logo\">\n    <nav class=\"navbar navbar-expand-lg